# store any upgrades inside the armor stands head item and check if there is any, don't overwrite existing upgrades
execute if block ~ ~ ~ barrel{Items:[{Slot:0b,components:{"minecraft:custom_data":{speed_mod:1b}},count:1}]} unless data entity @e[type=minecraft:armor_stand,tag=exp.mod_vehicle,predicate=expansion:compare_score/modstation,distance=..50,limit=1] ArmorItems[0].components.minecraft:custom_data.ModStorage.speed_mod.id store success score @s exp.bool run data modify entity @s ArmorItems[3].components.minecraft:custom_data.ModStorage.speed_mod set from block ~ ~ ~ Items[{Slot:0b}]
execute if block ~ ~ ~ barrel{Items:[{Slot:9b,components:{"minecraft:custom_data":{oxygen_tank:1b}},count:1}]} unless data entity @e[type=minecraft:armor_stand,tag=exp.mod_vehicle,predicate=expansion:compare_score/modstation,distance=..50,limit=1] ArmorItems[0].components.minecraft:custom_data.ModStorage.oxygen_tank.id store success score @s exp.bool run data modify entity @s ArmorItems[3].components.minecraft:custom_data.ModStorage.oxygen_tank set from block ~ ~ ~ Items[{Slot:9b}]